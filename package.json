{
  "name": "luxor-pokemon-adapter",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "author": "alexanderarmua1@gmail.com",
  "private": true,
  "engines": {
    "npm": "PLEASE USE YARN",
    "yarn": ">= 1.22.5",
    "node": ">= 16.16.0"
  },
  "scripts": {
    "start:all": "docker-compose up -d && yarn install && yarn migrate:prod && yarn prisma:generate && yarn dev",
    "dev": "nodemon",
    "worker": "npx ts-node -r tsconfig-paths/register ./src/workers/index.ts",
    "preinstall": "npx only-allow yarn",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start:prod": "node ./dist/index.js",
    "start:prod:worker": "node ./dist/workers/index.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-report": "eslint --output-file ./eslint-report.json --format json src --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --config .prettierrc \"{src,apps,libs,test}/**/*.ts\" --write",
    "prettier-fix": "node_modules/.bin/prettier -l \"{src,apps,libs,test}/**/*.ts\" --write",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:studio": "npx prisma studio",
    "migrate:dev": "npx prisma migrate dev --create-only --name",
    "migrate:reset": "npx prisma migrate reset",
    "migrate:prod": "npx prisma migrate deploy",
    "migrate:rollback": "npx prisma migrate resolve --rolled-back",
    "test": "jest --maxWorkers=2 --silent",
    "test:watch": "jest --watch --maxWorkers=2 --silent",
    "test:cov": "jest --coverage --maxWorkers=2 --silent",
    "prepare": "husky install",
    "docker:dev": "docker-compose -f docker-compose.yml up"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.282.0",
    "@aws-sdk/client-sqs": "^3.276.0",
    "@hapi/boom": "^10.0.1",
    "@prisma/client": "^4.4.0",
    "@types/compression": "^1.7.2",
    "@types/qrcode": "^1.5.1",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "aws-sdk": "^2.1285.0",
    "axios": "^1.4",
    "axios-cache-interceptor": "^1.2.0",
    "body-parser": "^1.20.2",
    "class-validator": "^0.13.2",
    "close-with-grace": "^1.1.0",
    "compression": "^1.7.4",
    "dataloader": "^2.1.0",
    "dd-trace": "^3.3.1",
    "dotenv": "^16.0.3",
    "eventemitter2": "^6.4.9",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.1",
    "graphql": "^15.3.0",
    "graphql-request": "^6.1.0",
    "imgix-management-js": "^1.3.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.8",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.1",
    "prisma": "^4.4.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sqs-consumer": "^7.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "type-graphql": "^1.1.1",
    "unleash-client": "^3.20.0",
    "uuid": "^9.0.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/axios-curlirize": "^1.3.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.1",
    "@types/mock-req-res": "^1.1.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.23",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.1.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.0.3",
